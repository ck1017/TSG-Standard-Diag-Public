        -------------------------------------------------------------------------------
-- This log is generated once at the start of a test
deviceName OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "Human-readable name of the device"
::= {debugConfig 1}

-- This log is generated once at the start of a test
mibVersion OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "The MIB version that the device supports"
::= {debugConfig 2}

