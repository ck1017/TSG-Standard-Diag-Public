        Type1Dci0 (1),
        Type1Dci1A2B2C (2),
        Type1Dci4 (3)
    }

    CyclicShift ::= INTEGER (0..7)

    SrsPowerOffset ::= INTEGER {
        Offset0 (0),
        Offset1 (1)
    }

