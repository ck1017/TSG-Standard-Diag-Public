    SYNTAX  Bandwidth
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "Downlink Bandwidth"
::= {lteRrcKpis 3}

    Bandwidth ::= INTEGER {
        Bandwidth-1-4-Mhz (1),  --1.4 MHz
        Bandwidth-3-Mhz (2),    --3 MHz
        Bandwidth-5-Mhz (3),    --5 MHz
        Bandwidth-10-Mhz (4),   --10 MHz
        Bandwidth-15-Mhz (5),   --15 MHz
        Bandwidth-20-Mhz (6)    --20 MHz
    }

