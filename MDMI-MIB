::= {wifiDebugEvents 6}

WiFiDeAuthenticationEvent ::= SEQUENCE {
    bssid   OCTET STRING,               -- MAC Address of the Access Point UE authenticated with
    reasonCode  INTEGER (0..66)         -- Refer to IEEE 802.11-8.4.1.7
}

